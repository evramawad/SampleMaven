pipeline {
    agent {
		label ''
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
		post {
   		 always {
			 cucumber fileIncludePattern: 'cucumber.json'
    }
}
}
}
}
