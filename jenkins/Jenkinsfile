pipeline {
    agent {
		label ''
    }
    stages {
        stage('Build') {
            steps {
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
		always {
			cucumber '--format json -o ${workspace}/cucumber.json'
		}
        }
    }
}
